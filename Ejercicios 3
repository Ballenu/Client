numbers2 = [2, 4, 5];

// 1 y 2 con una función conjunta (reduce), reduce los elementos de un array a un valor
function reduce(xs, f){
    var y = 0;
    for(var i = 0; i < xs.length; i++){
        y = f(xs[i]);
    }
    return y;
}

//1. Función que dados los elementos de un array los sume [1, 2, 3] => 6
function sumatory(x){
    return y += x;
}

console.log(reduce(numbers2, sumatory));

//2. Función que dados los elementos de un array devuelva el máximo [1, 2, 3] => 3
function max(x){
    if(y < xs[i]) return y = xs[i];
}

console.log(reduce(numbers2, max));

//3. Función que reciba un array de números y que devuelva cuántos son pares [2, 3, 4] => 2
